{{#if RUNTIME_WEB}}
import {FabaWebMediator} from "@fabalous/runtime-web/FabaWebMediator";
import {{MODULE_COMMAND}} from "./command/{{MODULE_COMMAND}}";
{{/if}}
{{#if RUNTIME_NODE}}
import {FabaNodeMediator} from "@fabalous/runtime-node/FabaNodeMediator";
import {{MODULE_SERVICE}} from "./service/{{MODULE_SERVICE}}";
{{/if}}
import {{MODULE_EVENT}} from "./event/{{MODULE_EVENT}}";
{{#if RUNTIME_WEB}}
export default class {{MODULE_MEDIATOR}} extends FabaWebMediator {
{{/if}}
{{#if RUNTIME_NODE}}
export default class {{MODULE_MEDIATOR}} extends FabaNodeMediator {
{{/if}}
    registerCommands():void {
        {{#if RUNTIME_WEB}}
        this.addCommand({{MODULE_EVENT}}, {{MODULE_COMMAND}});
        {{/if}}
        {{#if RUNTIME_NODE}}
        this.addCommand({{MODULE_EVENT}}, {{MODULE_SERVICE}});
        {{/if}}
    }
}